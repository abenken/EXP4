#include <iostream>
#include <conio.h>

using namespace std;

int main()
{
    int swappingvalue;
    int VariaA;
    int VariaB;
	
	int arr[x];
	
	cout << "Input array size: "; cin>> VariaA;
	cout << "Input array values" << endl;
	
	for(int i = 0; i < VariaA; i++)
	{
	cin>>arr[i];
	}
	for(int c = VariaB - 1; c > 0; c--)
	{
	for(int i = 0; i < VariaA; i++)
	{
	    if(arr[i] > arr[i + 1])
	    {
	    swappingvalue = arr[i + 1];
	    arr[i + 1] = arr[i];
	    arr[i] = swappingvalue;
	    }
	}  
	
	}	
	for(int i = 0; i < VariaA; i++)
	{
	cout<<arr[i]<<", ";
	}
	    
_getch();    
